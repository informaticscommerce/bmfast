<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <title>Packing Slips</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Packing Slips</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Mage4\PackingSlips\Block\Order\PackingSlipsList" name="customer_account_dashboard_packing"
                   template="Mage4_PackingSlips::slips.phtml" cacheable="false" />
        </referenceContainer>
    </body>
</page>
